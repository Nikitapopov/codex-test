    "eslint": "^8.1.0",
    "sequelize-cli": "^6.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "log4js": "^6.3.0",
    "multer": "^1.4.3",
